{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": [2, "never"],
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-trailing-spaces": false
  },
  "globals": {
    "jasmine": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "spyOn"
  }
}
